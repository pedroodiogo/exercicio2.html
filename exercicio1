function contarOcorrencias(numeros) {
  let resultado = {};
  
  for (let numero of numeros) {
    if (resultado[numero]) {
      resultado[numero]++;
    } else {
      resultado[numero] = 1;
    }
  }
  
  return resultado;
}

// Exemplo de uso:
let array = [1, 2, 3, 2, 4, 1, 5, 1];
console.log(contarOcorrencias(array));
